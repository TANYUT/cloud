<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.oauth2.mapper.ClientdetailsMapper">
    <resultMap id="clientdetailsMap" type="com.cloud.oauth2.entity.Clientdetails">
        <id property="appId" column="app_id"/>
        <result property="resourceIds" column="resource_ids"/>
        <result property="appSecret" column="app_secret"/>
        <result property="scope" column="scope"/>
        <result property="grantTypes" column="grant_types"/>
        <result property="redirectUrl" column="redirect_url"/>
        <result property="authorities" column="authorities"/>
        <result property="accessTokenValidity" column="access_token_validity"/>
        <result property="refreshTokenValidity" column="refresh_token_validity"/>
        <result property="additionalinformation" column="additionalInformation"/>
        <result property="autoApproveScopes" column="auto_approve_scopes"/>
    </resultMap>
</mapper>
