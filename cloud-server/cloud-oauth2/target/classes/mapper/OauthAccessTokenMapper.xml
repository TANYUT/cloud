<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.oauth2.mapper.OauthAccessTokenMapper">

    <resultMap id="oauthAccessTokenMap" type="com.cloud.oauth2.entity.OauthAccessToken">
        <id property="authenticationId" column="authentication_id"/>
        <result property="tokenId" column="token_id"/>
        <result property="token" column="token"/>
        <result property="userName" column="user_name"/>
        <result property="clientId" column="client_id"/>
        <result property="authentication" column="authentication"/>
        <result property="refreshToken" column="refresh_token"/>
    </resultMap>
</mapper>
